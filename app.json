{
  "expo": {
    "name": "crumb",
    "slug": "crumb",
    "owner": "crumbsbbangsaeng",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "crumb",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "fonts": [
      {
        "asset": "./assets/fonts/SUIT-Regular.ttf",
        "name": "Regular"
      },
      {
        "asset": "./assets/fonts/SUIT-SemiBold.ttf",
        "name": "SemiBold"
      },
      {
        "asset": "./assets/fonts/SUIT-Bold.ttf",
        "name": "Bold"
      }
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.seoyeon241.crumb"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ed06a817-0ffe-4bbc-8ec0-c7a9c412b12f"
      }
    }
  }
}
